{
    "name": "drive-embedder",
    "version": "1.0.15",
    "description": "Upload local files to Google Drive and embed them in Obsidian notes",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "google-drive",
        "embedder",
        "file-upload",
        "audio",
        "video",
        "pdf"
    ],
    "author": "reallygood83",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "^5.0.0",
        "esbuild": "^0.27.3",
        "obsidian": "latest",
        "tslib": "^2.8.1",
        "typescript": "4.7.4"
    },
    "dependencies": {}
}
